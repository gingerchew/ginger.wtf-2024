:root[data-theme="green"]:not([data-mode="dark"]) {
    --textColor: #070901;
    --primaryColor: #1f624f;
    --focusColor: #3f8071;
    --backgroundColor: #daf3ec;
    --complementColor: #011d18;
    --accentColor: #1f624f;
    --accentColorTransparent: #1f624f22;
}

:root[data-theme="green"][data-mode="dark"] {
    --textColor: #d8e7e4;
    --primaryColor: #240ba2;
    --focusColor: #765df4;
    --backgroundColor: #011d18;
    --complementColor: #6d7a76;
    --accentColor: #daf3ec;
    --accentColorTransparent: #daf3ec22;
}

:root[data-theme="arc"]:not([data-mode="dark"]) {
    --backgroundColor: var(--arc-palette-maxContrastColor);
    --primaryColor: var(--arc-palette-cutoutColor);
    --complementColor: var(--arc-palette-background);
    --focusColor: var(--arc-palette-focus);
    --textColor: var(--arc-palette-background);
    --accentColor: var(--arc-palette-minContrastColor);
    --accentColorTransparent: var(--arc-palette-focus);
    --complementColorRGB: var(--_complementColorRGB);
}

:root[data-theme="arc"][data-mode="dark"] {
    --backgroundColor: var(--arc-palette-background);
    --textColor: var(--arc-palette-title);
    --complementColor: var(--arc-palette-subtitle);
    --accentColor: var(--arc-palette-maxContrastColor);
    --accentColorTransparent: var(--arc-palette-focus);
}